var spectator;
socket.on('startSpectator', function(setup){
    if(!spectator) {
        spectator = new FagskakSpectator(container, socket, setup);
    } else {
        console.error("Spectator already initialized")
    }
})
        

socket.emit("startSpectator", "true");
</script>
